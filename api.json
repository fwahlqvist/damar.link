{
  "openapi": "3.0.0",
  "info": {
    "title": "damar link",
    "description": "API for https://damar.link",
    "contact": {
      "name": "Fredrik Wahlqvist",
      "url": "https://www.damar.link/contact",
      "email": ""
    },
    "license": {
      "name": "Apache 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0"
    },
    "version": "1.0.0"
  },
  "paths": {
    "/links": {
      "get": {
        "summary": "List all links",
        "responses": {
          "200": {
            "description": "List of links",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Link"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create a new link",
        "responses": {
          "201": {}
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemasLink"
              }
            }
          },
          "required": true
        }
      }
    },
    "/links/{linkId}": {
      "get": {
        "summary": "Get the link",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasLink"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Update the link",
        "responses": {
          "204": {}
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemasLink"
              }
            }
          },
          "required": true
        }
      },
      "delete": {
        "summary": "Delete the link",
        "responses": {
          "204": {}
        }
      },
      "parameters": [
        {
          "name": "linkId",
          "in": "path",
          "description": "The ID of the link",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ]
    },
    "/links/{linkName}": {
      "get": {
        "summary": "Get the link",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasLink"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Update the link by name",
        "responses": {
          "204": {}
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemasLink"
              }
            }
          },
          "required": true
        }
      },
      "delete": {
        "summary": "Delete the link",
        "responses": {
          "204": {}
        }
      },
      "parameters": [
        {
          "name": "linkName",
          "in": "path",
          "description": "The name of the link",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ]
    },
    "/links/{linkUrl}": {
      "get": {
        "summary": "Get the link",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Link"
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Update the link",
        "responses": {
          "200": {}
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/Link"
                }
              }
            }
          },
          "required": true
        }
      },
      "delete": {
        "summary": "Delete the link",
        "responses": {
          "204": {}
        }
      },
      "parameters": [
        {
          "name": "linkUrl",
          "in": "path",
          "description": "The URL of the link",
          "required": true,
          "schema": {
            "type": "string"
          }
        }
      ]
    }
  },
  "components": {
    "schemas": {
      "Link": {
        "title": "Root Type for Link",
        "description": "The root of the Link type's schema.",
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "method": {
            "type": "string"
          },
          "userId": {
            "type": "string"
          },
          "createdAt": {
            "type": "string"
          },
          "lastUpadtedAt": {
            "type": "string"
          },
          "responseCode": {
            "type": "string"
          },
          "responseCodeLastChecked": {
            "type": "string"
          },
          "analyticsProfileId": {
            "type": "string"
          },
          "webhooks": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          },
          "clicks": {
            "type": "string"
          }
        }
      }
    }
  }
}